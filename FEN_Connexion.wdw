     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // SwitchToLoginMode ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure SwitchToLoginMode()
      gbInscriptionMode					= Faux
      
      // Masquer les champs d'inscription
      SAI_Password_Confirmation.Visible	= Faux
      
      // Repositionner les contrôles
      PositionControls()
      
      // Changer les textes
      BTN_Login.Libellé		= "SE CONNECTER"
      LBL_SwitchMode.Libellé	= "Pas de compte ? S'inscrire"
      
      DemandeMiseAJourUI()
