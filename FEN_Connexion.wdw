   -
     name : LoadLastLoginData
     procedure_id : 1836088095174667371
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // LoadLastLoginData ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure LoadLastLoginData()
      
      
      // Charger le dernier nom d'utilisateur depuis les paramètres
      sEmail  est une chaîne = ChargeParamètre("auth_email", "")
      SI sEmail <> "" ALORS
      	SAI_Email = sEmail
      	// Mettre le focus sur le mot de passe si username présent
      	DonneFocus(SAI_Password)
      SINON
      	// Mettre le focus sur le username
      	DonneFocus(SAI_Email)
      FIN
     type : 458752
