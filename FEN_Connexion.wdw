   -
     name : ValidateInputs
     procedure_id : 1836099588509827905
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // ValidateInputs ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure ValidateInputs()
      
      // Vérifier l'email (toujours obligatoire)
      SI SAI_Email = "" ALORS
      	ToastAffiche("Veuillez saisir votre email.")
      	DonneFocus(SAI_Email)
      	RENVOYER Faux
      FIN
      
      //SI PAS ValidateEmail(SAI_Email) ALORS
      //	ToastAffiche("Format d'email invalide.")
      //	DonneFocus(SAI_Email)
      //	RENVOYER Faux
      //FIN
      
      // Vérifier le mot de passe
      SI SAI_Password = "" ALORS
      	ToastAffiche("Veuillez saisir votre mot de passe.")
      	DonneFocus(SAI_Password)
      	RENVOYER Faux
      FIN
      
      SI Taille(SAI_Password) < PASSWORD_MIN_LENGTH ALORS
      	ToastAffiche("Le mot de passe doit contenir au moins " + PASSWORD_MIN_LENGTH + " caractères.")
      	DonneFocus(SAI_Password)
      	RENVOYER Faux
      FIN
      
      // Validation spécifique au mode inscription
      SI gbInscriptionMode ALORS
      	// Vérifier le nom complet
      	SI SAI_Password_Confirmation = "" ALORS
      		ToastAffiche("Veuillez saisir la confirmation du mot de passes.")
      		DonneFocus(SAI_Password_Confirmation)
      		RENVOYER Faux
      	FIN
      
      	SI SAI_Password <> SAI_Password_Confirmation ALORS
      		{
      			ToastAffiche("les mots de passe ne sont pas identique.")
      			DonneFocus(SAI_Password_Confirmation)
      			RENVOYER Faux
      		}
      FIN
      
      RENVOYER Vrai
     type : 458752
   -
     name : ShowLoading
     procedure_id : 1836099657229844647
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ShowLoading (<bShow> est booléen)
      //
      // Paramètres :
      //	bShow (booléen) : <indiquez ici le rôle de bShow>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure ShowLoading(bShow est un booléen)
      PROG_Loading.Visible		= bShow
      BTN_Login..Visible			= PAS bShow
      
      SI bShow ALORS
      	BTN_Login.Libellé = "CONNEXION..."
      SINON
      	SI gbInscriptionMode ALORS
      		BTN_Login.Libellé = "S'INSCRIRE"
      	SINON
      		BTN_Login.Libellé = "SE CONNECTER"
      	FIN
      FIN
      
      DemandeMiseAJourUI()
     type : 458752
   -
     name : PerformRegistration
     procedure_id : 1836099773194513271
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // PerformRegistration ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure PerformRegistration()
      
      // Vérifier la connectivité
      SI PAS gConfig.CanConnect() ALORS
      	ShowLoading(Faux)
      	SI gConfig.IsWiFiOnly ALORS
      		ToastAffiche(MSG_ERR_WIFI_REQUIRED)
      	SINON
      		ToastAffiche(MSG_ERR_NETWORK)
      	FIN
      	RETOUR
      FIN
      
      // Lancer l'inscription en arrière-plan
      ThreadExécute("registration", threadSécurisé, CallbackRegistration,SAI_Email, SAI_Password)
     type : 458752
   -
     name : PerformLogin
     procedure_id : 1836099876274931695
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // PerformLogin ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure PerformLogin()
      
      
      // Vérifier la connectivité
      SI PAS gConfig.CanConnect() ALORS
      	ShowLoading(Faux)
      	SI gConfig.IsWiFiOnly ALORS
      		ToastAffiche(MSG_ERR_WIFI_REQUIRED)
      	SINON
      		ToastAffiche(MSG_ERR_NETWORK)
      	FIN
      	RETOUR
      FIN
      
      ThreadExécute("LOGIN",threadSécurisé,CallbackLogin,SAI_Email,SAI_Password)
     type : 458752
   -
     name : CallbackLogin
     procedure_id : 1836100091024001417
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // CallbackLogin ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure CallbackLogin(sEmail est une chaîne, sPassword est une chaîne)
      bSuccess est un booléen = gAuth.Login(sEmail, sPassword)
      ExécuteThreadPrincipal(OnLoginResult, bSuccess,sEmail)
      ThreadDemandeArrêt("LOGIN")
     type : 458752
   -
     name : OnLoginResult
     procedure_id : 1836100262824306246
     type_code : 14
     code : |1-
      procédure OnLoginResult(bSuccess est un booléen, sEmail est une chaîne)
      
      ShowLoading(Faux)
      
      SI bSuccess ALORS
      	// Sauvegarder l'email dans les paramètres
      	SauveParamètre("last_email", sEmail)
      
      	// Afficher un toast de succès
      	ToastAffiche("Connexion réussie !",toastCourt,cvMilieu,chCentre)
      	WriteLogMobile("Connexion réussie !")
      	// Ouvrir la fenêtre de chat
      	OuvreFenêtreMobile(FEN_UserConfig)
      SINON
      	// Afficher l'erreur
      	ToastAffiche(gAuth.GetLastError)
      	WriteLogMobile(gAuth.GetLastError)
      	WriteLogMobile(gAuth.GetLastError)
      
      	// Remettre le focus sur le mot de passe
      	SAI_Password = ""
      	DonneFocus(SAI_Password)
      FIN
     type : 458752
   -
     name : CallbackRegistration
     procedure_id : 1841238916296621675
     type_code : 14
     code : |1+
      //!
      //! @brief <indiquez ici ce que fait la procédure>
      //!
      //!
      procédure CallbackRegistration(sEmail est une chaîne, sPassword est une chaîne)
      
      bSuccess est un booléen = gAuth.Register(sEmail, sPassword)
      
      // Retour au thread principal
      ExécuteThreadPrincipal(OnRegistrationResult,bSuccess, sEmail)
      
     type : 458752
   -
     name : OnRegistrationResult
     procedure_id : 1841239178289819146
     type_code : 14
     code : |1+
      procédure OnRegistrationResult(bSuccess est un booléen, sEmail est une chaîne)
      
      
      ShowLoading(Faux)
      
      SI bSuccess ALORS
      	// Afficher un toast de succès
      	ToastAffiche("Inscription réussie ! Vous pouvez maintenant vous connecter.")
      
      	// Basculer vers le mode connexion
      	SwitchToLoginMode()
      
      	// Garder l'email rempli
      	SAI_Email		= sEmail
      	SAI_Password	= ""
      	DonneFocus(SAI_Password)
      SINON
      	// Afficher l'erreur
      	ToastAffiche(gAuth.GetLastError)
      FIN
     type : 458752
   -
     name : SwitchToRegistrationMode
     procedure_id : 1842900836722892480
     type_code : 14
     code : |1+
      //!
      //! @brief <indiquez ici ce que fait la procédure>
      //!
      //!
      procédure SwitchToRegistrationMode()
      gbInscriptionMode					= Vrai
      
      // Afficher les champs d'inscription
      SAI_Email.Visible					= Vrai
      SAI_Password_Confirmation.Visible	= Vrai
      
      // Repositionner les contrôles
      RepositionControls()
      
      // Changer les textes
      BTN_Login.Libellé			= "S'INSCRIRE"
      LBL_SwitchMode.Libellé		= "Déjà un compte ? Se connecter"
      
      DemandeMiseAJourUI()
     type : 458752
