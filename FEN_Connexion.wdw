      procédure ConfigureMaterialDesign()
      
      // Configuration des champs de saisie
      //SAI_Email.CouleurFond		= RVB(250, 250, 250)
      SAI_Email.Police.Nom							= "Roboto"
      SAI_Email.Police.Taille							= 10
      SAI_Email..Indication							= "Nom d'utilisateur"
      
